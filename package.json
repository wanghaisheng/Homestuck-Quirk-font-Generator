{
    "name": "homestuck-quirk-generator",
    "version": "5.0.0",
    "type": "module",
    "description": "A site that allows you to convert inputted text into the typing quirk of a Homestuck character.",
    "repository": {
        "type": "git",
        "url": "git+https://MergeCommits@github.com/MergeCommits/Homestuck-Quirk-Generator.git"
    },
    "homepage": "https://homestuck-quirks.web.app/",
    "bugs": {
        "url": "https://github.com/MergeCommits/Homestuck-Quirk-Generator/issues"
    },
    "author": "Mark Jarjour",
    "license": "SEE LICENSE IN LICENSE.md",
    "private": true,
    "packageManager": "pnpm@9.0.6",
    "scripts": {
        "build": "tsc && vite build",
        "dev": "vite",
        "lint": "eslint \"src/**/*{.ts,.tsx}\"",
        "postinstall": "cp .gitignore .prettierignore",
        "prepare": "husky",
        "spell-check": "cspell '**'",
        "start": "vite preview"
    },
    "devDependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@fontsource/roboto": "^5.0.13",
        "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
        "@mui/icons-material": "^5.15.15",
        "@mui/material": "^5.15.15",
        "@nabla/vite-plugin-eslint": "^2.0.4",
        "@tsconfig/vite-react": "^3.0.2",
        "@types/node": "^20.12.7",
        "@types/react": "^18.3.1",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.8.0",
        "@typescript-eslint/parser": "^7.8.0",
        "@vitejs/plugin-react": "^4.2.1",
        "cspell": "^8.7.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-no-relative-import-paths": "^1.5.4",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "prettier-plugin-pkg": "^0.18.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "typescript": "^5.4.5",
        "vite": "^5.2.10",
        "vite-plugin-svgr": "^4.2.0",
        "vite-tsconfig-paths": "^4.3.2"
    },
    "lint-staged": {
        "*.{ts,tsx}": "eslint --cache --fix",
        "*.{ts,tsx,css,json,md}": [
            "prettier --write",
            "cspell --cache"
        ]
    }
}
